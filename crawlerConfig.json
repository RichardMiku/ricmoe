{
  "index_name": "ricmoesearch",
  "start_urls": ["https://www.ric.moe/"],
  "rateLimit": 8,
  "maxDepth": 10,
  "pathsToMatch": [
    "https://www.ric.moe/**"
  ],
  "recordExtractor": {
    "lvl0": {
      "selectors": ".menu__link.menu__link--sublist.menu__link--active, .navbar__item.navbar__link--active",
      "defaultValue": "Documentation",
      "last": true
    },
    "lvl1": [
      "header h1",
      "article h1"
    ],
    "lvl2": "article h2",
    "lvl3": "article h3",
    "lvl4": "article h4",
    "lvl5": "article h5, article td:first-child",
    "lvl6": "article h6",
    "content": "article p, article li, article td:last-child"
  }
}